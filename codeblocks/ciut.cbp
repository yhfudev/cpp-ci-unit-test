<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ciut" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/ciut" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/ciut" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DCIUT_ENABLED=1" />
			<Add directory="../include" />
		</Compiler>
		<Unit filename="../examples/ciutexcpp.cpp" />
		<Unit filename="../examples/test-exception.cpp" />
		<Unit filename="../examples/test-lang.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../examples/test-library.cpp" />
		<Unit filename="../examples/test-sigsegv.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../include/ciut-sio.h" />
		<Unit filename="../include/ciut.h" />
		<Unit filename="../include/uclog.h" />
		<Unit filename="../include/ucport.h" />
		<Unit filename="../include/uctime.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
